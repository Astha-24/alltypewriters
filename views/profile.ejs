<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Let browser know website is optimized for mobile
      <meta name="viewport" content="width=device-width, initial-scale=1.0"/>-->
    <title>All TYPE WRITERS</title>
    <!-- Bootstrap Core CSS -->
    <link rel="stylesheet" href="bootstrap/css/bootstrap.css" type="text/css">
    <!-- load bootstrap css -->
    <link rel="stylesheet" href="bootstrap/css/bootstrap-theme.css" type="text/css">
    <!-- load bootstrap-theme css -->
    <!-- Theme CSS -->
    <link href="index/css/freelancer.css" rel="stylesheet">
    <!-- Custom Fonts -->
    <link href="index/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Bree+Serif" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Arvo" rel="stylesheet">
    <!--BOX for displayinf categories css-->
    <link rel="stylesheet" href="box.css" type="text/css">
    <!-- Sidebartheme-->
    <style>
    .img-profile {
        padding-top: 0px;
        padding-bottom: 0px;
        border-radius: 50%;
        height: 50px;
        width: 65px;
    }

    .container-fluid {
        padding-top: 200px;
        padding-bottom: 100px;
        text-align: center;
    }

    .unsungheroes {
        height: 400px;
        border: 2px solid #e8e8e8;
        box-shadow: 0 0 10px #e8e8e8;
        margin-left: 50px;
        margin-right: 50px;
        margin-top: 25px;
        transition: 1s ease;

        background-image: url('assets/img/hero.jpeg');
        background-size: 100% 280px;

        background-repeat: no-repeat;
    }

    .unsungheroes:hover {

        
        -webkit-transform: scale(1.1);
        -ms-transform: scale(1.1);
        transform: scale(1.1);
        transition: 1s ease;
    }

    .underthesun {
        height: 400px;
        border: 2px solid #e8e8e8;
        box-shadow: 0 0 10px #e8e8e8;
        margin-left: 50px;
        margin-right: 50px;
        margin-top: 25px;
        transition: 1s ease;

        background-image: url('assets/img/sun.jpg');
        background-size: 100% 280px;
        background-blend-mode;

        background-repeat: no-repeat;
    }

    .underthesun:hover {

        
        -webkit-transform: scale(1.1);
        -ms-transform: scale(1.1);
        transform: scale(1.1);
        transition: 1s ease;
    }

    .beyondheadlines {
        height: 400px;
        border: 2px solid #e8e8e8;
        box-shadow: 0 0 10px #e8e8e8;
        padding-right: 10px;
        margin-left: 50px;
        margin-right: 50px;
        margin-top: 25px;
        transition: 1s ease;

        background-image: url('assets/img/newspaper.jpeg');
        background-size: 100% 280px;

        background-repeat: no-repeat;
    }

    .beyondheadlines:hover {

        
        -webkit-transform: scale(1.1);
        -ms-transform: scale(1.1);
        transform: scale(1.1);
        transition: 1s ease;
    }

    .hero {
        background-image: url('assets/img/hero.jpeg');
        background-size: 100%;
        height: 250px;
        background-repeat: no-repeat;
    }

    .btn-circle.btn-lg {
        width: 50px;
        height: 50px;
        padding: 10px 16px;
        font-size: 18px;
        line-height: 1.33;
        border-radius: 25px;
        margin-top: 255px;
        margin-left: 200px;
    }
    .btn-circle.btn-lg:hover{
        cursor: pointer;
    }
    .categoriesText{
        font-family: 'Bree Serif', serif;
    }

    </style>
</head>

<body id="page-top" class="index">
    <!--Import jQuery before materialize.js-->
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script type="text/javascript" src="js/materialize.min.js"></script>
    <div id="skipnav"><a href="#maincontent">Skip to main content</a></div>
    <!-- Navigation -->
    <nav id="mainNav" class="navbar navbar-default navbar-fixed-top navbar-custom">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header page-scroll">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span> Menu <i class="fa fa-bars"></i>
                </button>
                <a class="navbar-brand" href="#page-top">All Type Writers</a>
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li class="hidden">
                        <a href="#page-top"></a>
                    </li>
                    <%if (user.facebook.photo) {%>
                        <li class="page-scroll">
                            <a href="/profile/<%=user._id%>" style="padding-top: 0px;padding-bottom: 0px;"> 
                                <img class=" img-responsive img-profile"src="<%=user.facebook.photo%>">
                        </a>
                        </li>
                        <%}else{%>
                            <li class="page-scroll">
                                <a href="/profile/<%=user._id%>">Profile</a>
                            </li>
                            <%}%>
                                <%if (user.facebook.name) {%>
                                    <li class="page-scroll">
                                        <a>Hi!  <%=user.facebook.name%></a>
                                    </li>
                                    <%}%>
                                        <%if (user.google.name) {%>
                                            <li class="page-scroll">
                                                <a>Hi!  <%=user.google.name%></a>
                                            </li>
                                            <%}%>
                                                <%if (user.local.name) {%>
                                                    <li class="page-scroll">
                                                        <a>Hi!  <%=user.local.name%></a>
                                                    </li>
                                                    <%}%>
                                                        <li class="page-scroll">
                                                            <a href="/profile/startastory" target="_blank">Start a story</a>
                                                        </li>
                                                        <li class="page-scroll">
                                                            <a href="/logout">Logout</a>
                                                        </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>
    <div class="container-fluid">
        <div class="col-lg-3 unsungheroes">
            <a href="/profile/cat/unsungheroes">
                <button type="button" class="btn btn-success btn-circle btn-lg"><i class="glyphicon glyphicon-chevron-right"></i></button>
            </a>
            <p class="categoriesText">
                Unsung Heroes
            </p>
        </div>
        <div class="col-lg-3 underthesun">
            <a href="/profile/cat/underthesun">
                <button type="button" class="btn btn-success btn-circle btn-lg"><i class="glyphicon glyphicon-chevron-right"></i></button>
            </a>            
            <p class="categoriesText">
                Under The Sun
            </p>
        </div>
        <div class="col-lg-3 beyondheadlines">
            <a href="/profile/cat/beyondheadlines">
                <button type="button" class="btn btn-success btn-circle btn-lg"><i class="glyphicon glyphicon-chevron-right"></i></button>
            </a>

            <p class="categoriesText">
                Beyond Headlines
            </p>
        </div>
    </div>
    <!-- jQuery -->
    <script src="assets/js/jquery-3.2.1.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="bootstrap/js/bootstrap.min.js"></script>
    <!-- Plugin JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
    <!-- Contact Form JavaScript -->
    <script src="index/js/jqBootstrapValidation.js"></script>
    <script src="index/js/contact_me.js"></script>
    <!-- Theme JavaScript -->
    <script src="index/js/freelancer.min.js"></script>
